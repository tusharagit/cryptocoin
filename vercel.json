{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@frontity/now"
      }
    ],
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          {
            "key": "Access-Control-Allow-Origin",
            "value": "*"
          }
        ]
      }
    ]    
  }